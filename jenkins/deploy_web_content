node {
  def toDeploy = params.CONTENT.split(",").collect{"\"${it}\""}.join(",").replace(" ","")
  
  
  stage('checkout'){
    checkout scm
  }
  stage('listing'){
    sh "ls -l"
    sh "whoami"
  }
  stage('test ansible'){
    sh """ansible-playbook copy_web_files.yml -e 'ansible_ssh_user=ecorella' -e '{ \"content\" : [\"${fileToDeploy}\"]}'"""
  }
}
