node {
  def fileToDeploy = params.CONTENT
  
  println "$fileToDeploy"
  
  stage('checkout'){
    checkout scm
  }
  stage('listing'){
    sh "ls -l"
    sh "whoami"
  }
  stage('test ansible'){
    sh """ansible-playbook copy_web_files.yml -e 'ansible_ssh_user=ecorella' -e '{ \"content\" : [\"${fileToDeploy}\"]}'"""
  }
}
